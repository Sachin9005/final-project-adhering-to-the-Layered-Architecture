	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MY_Project -  CarRental"/>
	<parameter name="PAYMENT_ID" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[SELECT
    bp.first_payment_id                 AS payment_id,
    DATE_FORMAT(bp.base_payment_date, '%Y-%m-%d') AS payment_date,

    c.name                              AS customer_name,
    c.nic_or_passport_number            AS customer_id,
    c.phone_number                      AS customer_contact,

    v.vehicle_No                         AS vehicle_no,
    v.model                              AS vehicle_model,
    v.type                                AS vehicle_type,
    v.rate_per_day          AS day_rate,

    DATE_FORMAT(r.start_DATE, '%Y-%m-%d')   AS rental_start,
    DATE_FORMAT(r.return_date, '%Y-%m-%d')  AS rental_end,
    r.dates_of_rent                        AS rental_days,

    bp.base_payment                       AS base_payment,
    bp.final_payment                      AS final_payment,

    d.name                                AS driver_name,
    d.phone_number                         AS driver_contact,
    d.license_number                       AS driver_license,
    d.driver_rate_per_day         AS driver_rate,

    rd.discount_amount_applied AS discount

FROM First_Payment bp
INNER JOIN Rental r
        ON bp.rental_id = r.rental_id
INNER JOIN Customer c
        ON r.customer_id = c.customer_id
INNER JOIN Vehicle v
        ON r.vehicle_id = v.vehicle_id
LEFT JOIN Rental_Discount rd
        ON r.rental_id = rd.rental_id
LEFT JOIN Driver d
       ON r.driver_id = d.driver_id

WHERE bp.first_payment_id = $P{PAYMENT_ID}]]></query>
