      </VBox>
      <VBox layoutX="46.0" layoutY="78.0" prefHeight="115.0" prefWidth="246.0">
         <children>
            <ImageView fitHeight="113.0" fitWidth="146.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets left="100.0" />
               </VBox.margin>
               <image>
                  <Image url="@images/cs.png" />
               </image>
            </ImageView>
         </children>
      </VBox>
      <VBox layoutX="275.0" layoutY="85.0" prefHeight="38.0" prefWidth="366.0">
