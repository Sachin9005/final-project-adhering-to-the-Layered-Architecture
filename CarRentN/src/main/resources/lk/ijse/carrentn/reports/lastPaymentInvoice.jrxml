		<expression><![CDATA[(($F{late_days}<=1) || ($F{driver_name} == null)) ? 0.0 : 300* $F{late_days}]]></expression>
	</variable>
	<variable name="TOTAL_PAYMENT" class="java.lang.Double">
		<expression><![CDATA[$V{VEHICLE_TOTAL}+$V{DRIVER_TOTAL}+$V{VEHICLE_LATE}+$V{DRIVER_LATE}+$P{VEHICLE_DAMAGE_FEE}]]></expression>
	</variable>
