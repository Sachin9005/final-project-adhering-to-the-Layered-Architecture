<jasperReport name="lastPaymentInvoice" language="java" pageWidth="595" pageHeight="842" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="bb10b521-07a2-4d9d-97f8-7639731c31d6">
	<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MY_Project -  CarRental"/>
	<parameter name="FINAL_PAYMENT_ID" class="java.lang.Integer"/>
	<parameter name="VEHICLE_DAMAGE_FEE" class="java.lang.Double"/>
	<parameter name="CUSTOMER_PAY" class="java.lang.Double"/>
	<query language="SQL"><![CDATA[SELECT
    fp.last_payment_id        AS invoice_id,
    fp.late_days     AS late_days,
    fp.balance_payment         AS  balancePay,
    fp.fine_payment AS fine,    
    DATE(fp.last_payment_date) AS invoice_date,

    c.name                     AS customer_name,
    c.nic_or_passport_number AS nic,
    c.phone_number             AS customer_contact,

    v.vehicle_no  AS vehicleNo,
    v.model AS vehicleModel,
    v.rate_per_day AS day_rate,

    r.dates_of_rent            AS rental_days,
    r.start_date                 AS start_date,
      r.return_date                 AS return_date,

    d.name                     AS driver_name,
    d.license_number AS driver_licenNo,
    d.driver_rate_per_day               AS driver_rate,

    rd.discount_amount_applied                AS discount,
    
    bp.base_payment          AS  base_payment,
    bp.final_payment          AS payment_of_first,
    bp.first_payment_id  AS first_pay_id

FROM last_Payment fp
JOIN Rental r      ON fp.rental_id = r.rental_id
JOIN Customer c    ON r.customer_id = c.customer_id
JOIN Vehicle v     ON r.vehicle_id = v.vehicle_id
LEFT JOIN Driver d ON r.driver_id = d.driver_id
LEFT JOIN Rental_Discount rd ON r.rental_id = rd.rental_id
JOIN First_Payment bp ON r.rental_id = bp.rental_id

WHERE fp.last_payment_id = $P{FINAL_PAYMENT_ID}
]]></query>
